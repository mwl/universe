{
  "id": "/{{humio.name}}",
  "instances": 1,
  "cpus": {{humio.cpus}},
  "mem": {{humio.mem}},
  "cmd": "SERVER_PORT=$PORT0 jre1.8.0_131/bin/java -jar scheduler-0.1-alpha-1.jar",
  "constraints": [
    [
      "hostname", "UNIQUE"
    ]
  ],
  "networks": [
    {
      "mode": "host"
    }
  ],
    "portDefinitions": [
      {
        "name": "default",
        "protocol": "tcp",
        "port": 0
      }
  ],
  "env": {
    "HUMIO_EXECUTOR_URL": "https://github.com/humio/dcos2humio/releases/download/0.1-alpha-1/executor-0.1-alpha-1.jar",
    "MESOS_COMMAND": "jre1.8.0_121/bin/java -jar executor-0.1-alpha-1.jar",
    "HUMIO_HOST": "{{humio.host}}",
    "HUMIO_DATASPACE": "{{humio.dataspace}}",
    "HUMIO_INGESTTOKEN": "{{humio.ingestToken}}"
  },
  "uris": [
    "{{resource.assets.uris.jre-tar-gz}}",
    "{{resource.assets.uris.scheduler-jar}}"
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 0
  }
}